    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch HTTP server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": ["api-server"],
            "buildFlags": "-tags=jsoniter,sqlite_rtree"
        },
        {
            "name": "Launch bulk loader",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "bulk-loader",
                "../street-manager-archive/permits",
                "--max-files",
                "10"
            ],
            "buildFlags": "-tags=jsoniter,sqlite_rtree"
        },
        {
            "name": "Launch regenerate index",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": ["regen", "--db=./data/street-manager.db"],
            "buildFlags": "-tags=jsoniter,sqlite_rtree"
        },
        {
            "name": "Launch delete completed events (dry-run)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "delete-completed",
                "--db=./data/street-manager.db"
            ],
            "buildFlags": "-tags=jsoniter,sqlite_rtree"
        }
    ]
}
